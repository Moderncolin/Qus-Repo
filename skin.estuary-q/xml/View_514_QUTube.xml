				<!-- <onright>531</onright> -->				
				<onup condition="Integer.IsGreater(Container.NumPages,1)">514</onup>
				<ondown condition="!Integer.IsGreater(Container.NumPages,1)">514</ondown>

                <viewtype label="QUTube">u icon</viewtype>
				<preloaditems>2</preloaditems>
				<pagecontrol>531</pagecontrol>
				<visible>Container.Content(artists) | Container.Content(albums) | Container.Content(sets) | Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(musicvideos) | Container.Content(images) | Container.Content(videos) | Container.Content(games)</visible>
<description>-----------------vV This is the U-Tube layout Vv--------------------</description>	
				<itemlayout height="300" width="350" condition="Container.Content(videos)">
					<control type="group">
						<left>64</left>
						<top>110</top>
						<include content="KodiQ-UtubeHorizontalLayout">
							<param name="focused" value="false" />
							<param name="thumb_label" value="$INFO[ListItem.Label2]" />
						</include>
					</control>
				</itemlayout>
				<focusedlayout height="300" width="300" condition="Container.Content(videos)">
					<control type="group">
						<left>64</left>
						<top>110</top>
						<include content="KodiQ-UtubeHorizontalFocusedLayout">
							<param name="focused" value="true" />
							<param name="thumb_label" value="$INFO[ListItem.Label2]" />
						</include>
					</control>
				</focusedlayout>	
			</control>			
		</control>		
		
	</include>

