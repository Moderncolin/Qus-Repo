			<include>Visible_Right</include>
            <include>OpenClose_Right</include> 

			<control type="panel" id="507">
				<left>460</left>
				<bottom>0</bottom>
				<right>15</right>
				<height>64%</height>
				
				<!-- <onleft>9000</onleft> -->
				<!-- <onright>531</onright> -->				
				<onup condition="Integer.IsGreater(Container.NumPages,1)">507</onup>
				<ondown condition="!Integer.IsGreater(Container.NumPages,1)">507</ondown>

                <viewtype label="QInfoWall">q icon</viewtype>
				<preloaditems>2</preloaditems>
				<pagecontrol>531</pagecontrol>
				<visible>Container.Content(artists) | Container.Content(albums) | Container.Content(sets) | Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(musicvideos) | Container.Content(images) | Container.Content(videos) | Container.Content(games)</visible>

               <description>-----------------vV This is the movie layout Vv--------------------</description>
				<itemlayout height="348" width="266" condition="Container.Content(movies) | Container.Content(sets) | Container.Content(tvshows) | Container.Content(seasons)">
					<control type="group">
						<left>68</left>
						<top>10</top>
						<include content="KodiQ-VerticalMoviesLayout"> 
							<param name="focused" value="false" />
						</include>
					</control>
				</itemlayout>
				<focusedlayout height="348" width="266" condition="Container.Content(movies) | Container.Content(sets) | Container.Content(tvshows) | Container.Content(seasons)">				    
					<control type="group">
						<left>0</left>
						<top>0</top>
						<include content="KodiQ-VerticalFocusedMoviesLayout"> 
							<param name="focused" value="true" />
						</include>
					</control>	
				</focusedlayout>
                <description>-----------------vV This is the episode layout Vv--------------------</description>	
				<itemlayout height="300" width="300" condition="Container.Content(episodes) | Container.Content(musicvideos)">
					<control type="group">
						<left>64</left>
						<top>110</top>
						<include content="KodiQ-HorizontalEpisodesLayout">
							<param name="focused" value="false" />
							<param name="thumb_label" value="$INFO[ListItem.Label2]" />
						</include>
					</control>
				</itemlayout>
				<focusedlayout height="300" width="300" condition="Container.Content(episodes) | Container.Content(musicvideos)">
					<control type="group">
						<left>64</left>
						<top>110</top>
						<include content="KodiQ-HorizontalEpisodesFocusedLayout">
							<param name="focused" value="true" />
							<param name="thumb_label" value="$INFO[ListItem.Label2]" />
						</include>
					</control>
				</focusedlayout>
                <description>-----------------vV This is the music layout Vv--------------------</description>
				<itemlayout height="348" width="266" condition="Container.Content(artists) | Container.Content(albums)">
					<control type="group">
						<left>68</left>
						<top>10</top>
						<include content="KodiQ-VerticalMusicLayout"> 
							<param name="focused" value="false" />
						</include>
					</control>
				</itemlayout>
				<focusedlayout height="348" width="266" condition="Container.Content(artists) | Container.Content(albums)">				    
					<control type="group">
						<left>0</left>
						<top>0</top>
						<include content="KodiQ-VerticalFocusedMusicLayout"> 
							<param name="focused" value="true" />
						</include>
					</control>	
				</focusedlayout>
                <description>-----------------vV This is the picture layout Vv--------------------</description>				
				<itemlayout height="400" width="378" condition="Container.Content(images)">
					<control type="group">
						<left>0</left>
						<top>150</top>
						<include>InfoWallPictureLayout</include>
					</control>
				</itemlayout>
				<focusedlayout height="400" width="378" condition="Container.Content(images)">
					<control type="group">
						<depth>DepthContentPopout</depth>
						<left>0</left>
						<top>150</top>
						<animation effect="zoom" start="100" end="110" time="200" tween="sine" easing="inout" center="180,330">Focus</animation>
						<animation effect="zoom" start="110" end="100" time="200" tween="sine" easing="inout" center="180,330">UnFocus</animation>
						<include content="InfoWallPictureLayout">
							<param name="focused" value="true" />
						</include>
					</control>
				</focusedlayout>
